import pandas as pd

# Load the CSV files into pandas DataFrames
df1 = pd.read_csv('Unpivot.csv')
df2 = pd.read_csv('SingleScores.csv')

# Merge the two DataFrames based on a common column
merged_df = pd.merge(df1, df2, on='Property/Plant ID')

# Save the merged DataFrame to a new CSV file
merged_df.to_csv('ES_Merged.csv', index=False)
